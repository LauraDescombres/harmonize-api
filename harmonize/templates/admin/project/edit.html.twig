{% extends "admin/base.html.twig" %}

{% block title %}Modifier le projet{% endblock %}

{% block body %}

<h2 class="text-center my-3">{{ 'Modification du projet : ' ~ project.name }}</h2>

{{ form_start(form) }}
    {{ form_rest(form) }}
    <div class="mt-2">
        <a href="{{ path('projects_browse') }}" class="btn btn-info">Annuler</a>
        <button class="btn btn-success">Modifier</button>
    </div>
{{ form_end(form)}}

{% endblock %}